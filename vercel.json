{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/configurator",
      "dest": "/configurator.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

