{
    "rewrites": [
      {
        "source": "/configurator.html",
        "destination": "/configurator.html"
      }
    ],
    "cleanUrls": true,
    "headers": [
      {
        "source": "/(.*).js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }
  